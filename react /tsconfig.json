{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "moduleResolution":"node"
  },
  "include": ["src/**/*", "config/webpack.config.ts"],
  "exclude": ["node_modules"],
  "ts-node": {
    "compilerOptions": {
        "module": "CommonJS"
     }
  }
 
}